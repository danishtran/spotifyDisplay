[Unit]
Description=Bring up Wi-Fi on boot
After=network.target

[Service]
Type=oneshot
ExecStart=/sbin/ip link set wlan0 up
ExecStart=/usr/sbin/wpa_supplicant -B -i wlan0 -c /etc/wpa_supplicant/wpa_supplicant-wlan0.conf
ExecStart=/sbin/udhcpc -i wlan0
ExecStart=/sbin/ip a show wlan0
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target

